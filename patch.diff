+package com.coocoo.admob
+
+import android.content.Context
+import androidx.lifecycle.CCMutableLiveData
+import com.coocoo.report.Report
+import com.google.android.gms.ads.AdListener
+import com.google.android.gms.ads.AdLoader
+import com.google.android.gms.ads.AdRequest
+import com.google.android.gms.ads.nativead.NativeAd
+
+class AdMobLoader(private val context: Context) {
+
+    fun load(adId: String, cachedNativeAd: CCMutableLiveData<NativeAd>, adListener: AdListener) {
+
+        val adLoader = AdLoader.Builder(context, adId).forNativeAd { nativeAd ->
+            cachedNativeAd.value?.destroy()
+            cachedNativeAd.value = nativeAd
+        }.withAdListener(adListener).build()
+
+        adLoader.loadAds(AdRequest.Builder().build(),5)
+    }
+}
