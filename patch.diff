+package com.coocoo.callhistory
+
+import X.cc_5WG
+import android.view.Gravity
+import android.view.View
+import android.view.ViewGroup
+import android.view.ViewParent
+import android.widget.RelativeLayout
+import com.coocoo.annotation.SmaliInject
+
+object CallHistoryDelegate {
+    @JvmStatic
+    @SmaliInject
+    fun getView(viewType: Int, view: View?, parent: ViewGroup): View? {
+        return if (viewType == CallHistoryItem.VIEW_TYPE) {
+            if (view == null) {
+                val banner = RelativeLayout(parent.context).apply {
+                    layoutParams = ViewGroup.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT, 200)
+                    gravity = Gravity.CENTER
+                }
+                val item = CallHistoryItem()
+                item.bindView(banner)
+                banner
+            } else {
+                view
+            }
+        } else {
+            null
+        }
+    }
+
+    @JvmStatic
+    @SmaliInject
+    fun addCallListBannerItem(arr: ArrayList<cc_5WG>) {
+        arr.add(CallHistoryItem())
+    }
+}
